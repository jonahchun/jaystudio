{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "chumper/zipper": "^1.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "paypal/rest-api-sdk-php": "^1.14",
        "wfn/admin": "^1.0",
        "wfn/cms": "^1.0",
        "wfn/customer": "^1.0",
        "wfn/emails": "^1.0",
        "wfn/navigation": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "PayPal\\": "vendor/paypal/lib/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "wfn-admin": {
            "type": "vcs",
            "url": "git@bitbucket.org:wanted-for-nothing/core-admin.git"
        },
        "wfn-customer": {
            "type": "vcs",
            "url": "git@bitbucket.org:wanted-for-nothing/customer.git"
        },
        "wfn-cms": {
            "type": "vcs",
            "url": "git@bitbucket.org:wanted-for-nothing/cms.git"
        },
        "wfn-navigation": {
            "type": "vcs",
            "url": "git@bitbucket.org:wanted-for-nothing/navigation.git"
        },
        "wfn-emails": {
            "type": "vcs",
            "url": "git@bitbucket.org:wanted-for-nothing/emails.git"
        }
    }
}
